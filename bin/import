#!/usr/bin/env node
require('babel/register');

var appPromise = require('../lib/app');
var restaurants = require('../public/fixtures/restaurants.json');

appPromise.then(function(app) {
  var service = app.service('api/restaurants');
  var count = 0;
  restaurants.forEach(function(restaurant) {
    service.create(restaurant, {}, function(err, created) {
      console.log('Created', created);
      count++;
      if(count === restaurant.length) {
        process.exit();
      }
    });
  });
});
