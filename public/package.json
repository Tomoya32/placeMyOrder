{
  "name": "place-my-order-frontend",
  "version": "0.1.0",
  "description": "The frontend for the restaurant order application",
  "main": "app/main",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "live-reload": "steal-tools live-reload"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "can": "git://github.com/bitovi/canjs#place-my-order2",
    "bit-tabs": "git://github.com/bitovi-components/bit-tabs#place-my-order2",
    "can-connect": "git://github.com/canjs/can-connect#place-my-order2",
    "event-emitter": "^0.3.3",
    "jquery": "^1.11.3",
    "socket.io-client": "^1.3.5",
    "steal": "^0.10.1"
  },
  "system": {
    "main": "app/main.stache!can/view/autorender/system",
    "transpiler": "babel",
    "meta": {
      "socketio": {
        "format": "global"
      }
    },
    "map": {
      "bit-tabs@0.0.4#tabs.less!$less": "@empty"
    },
    "paths": {
      "socketio": "node_modules/socket.io-client/socket.io.js"
    },
    "configDependencies": [
      "live-reload",
      "system-config"
    ],
    "bundle": [
      "app/home/home",
      "app/order/order",
      "app/restaurant/details/details",
      "app/restaurant/list/list",
      "app/order/history/history.component!can/component/system"
    ]
  },
  "devDependencies": {
    "steal-qunit": "0.0.4",
    "steal-tools": "^0.10.1"
  }
}
