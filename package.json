{
  "name": "place-my-order",
  "version": "0.1.0",
  "description": "The place-my-order frontend, built with DoneJS",
  "main": "pmo/layout.stache!done-autorender",
  "scripts": {
    "start": "node .server.js",
    "test": "testee test.html --browsers firefox --reporter Spec",
    "live-reload": "steal-tools live-reload"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "bit-tabs": "0.1.0-pre.0",
    "can": "2.3.0-pre.1",
    "can-connect": "git://github.com/canjs/can-connect#master",
    "can-ssr": "^0.1.1",
    "d3": "3.5.5",
    "done-autorender": "^0.0.7",
    "done-css": "^0.0.6",
    "jquery": "^1.11.3",
    "place-my-order-api": "^0.1.4",
    "socket.io-client": "^1.3.5",
    "steal": "^0.10.4",
    "system-component": "^0.0.3"
  },
  "devDependencies": {
    "steal-qunit": "0.0.4",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "steal-tools": "^0.10.4",
    "testee": "^0.2.0"
  },
  "system": {
    "main": "pmo/layout.stache!done-autorender",
    "transpiler": "babel",
    "meta": {
      "socketio": {
        "format": "global"
      },
      "can/util/vdom/vdom": {
        "sideBundle": true
      }
    },
    "map": {
      "bit-tabs@0.0.4#tabs.less!$less": "@empty",
      "$css": "done-css"
    },
    "paths": {
      "socketio": "node_modules/socket.io-client/socket.io.js"
    },
    "configDependencies": [
      "live-reload",
      "system-config"
    ],
    "bundle": [
      "pmo/home.stache!",
      "pmo/order/order",
      "pmo/restaurant/details/details.stache!",
      "pmo/restaurant/list/list",
      "pmo/order/history/history"
    ],
    "npmIgnore": [
      "testee", "place-my-order-api"
    ]
  }
}
