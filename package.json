{
  "name": "place-my-order",
  "version": "0.1.0",
  "description": "The place-my-order frontend, built with DoneJS",
  "main": "nw.html",
  "repository": "git@github.com:donejs/place-my-order.git",
  "scripts": {
    "document": "documentjs",
    "start": "node .server.js",
    "test": "testee test.html --browsers firefox --reporter Spec",
    "live-reload": "steal-tools live-reload",
    "build": "node build",
    "build:cordova": "node build cordova",
    "build:nw": "node build nw"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "bit-tabs": "^0.1.0-pre.1",
    "can": "^2.3.0-pre.9",
    "can-connect": "0.0.2",
    "can-ssr": "^0.5.7",
    "d3": "3.5.5",
    "done-autorender": "^0.3.0",
    "done-css": "^1.1.7",
    "es6-promise": "^2.2.0",
    "jquery": "^1.11.3",
    "place-my-order-api": "^0.4.2",
    "place-my-order-assets": "^0.1.0",
    "socket.io-client": "^1.3.5",
    "steal": "^0.11.0-pre.8",
    "steal-platform": "0.0.4",
    "done-component": "^0.2.0"
  },
  "devDependencies": {
    "documentjs": "^0.3.0-pre.5",
    "donejs-deploy": "^0.1.0",
    "funcunit": "^3.0.0",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "steal-cordova": "0.0.5",
    "steal-nw": "0.0.2",
    "steal-qunit": "0.0.4",
    "steal-tools": "^0.11.0-pre.11",
    "testee": "^0.2.0"
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "divshot",
          "environment": "production",
          "config": {
            "name": "matt-pmo",
            "headers": {
              "/**": {
                "Access-Control-Allow-Origin": "*"
              }
            }
          }
        }
      }
    }
  },
  "system": {
    "main": "pmo/index.stache!done-autorender",
    "transpiler": "babel",
    "envs": {
      "server-development": {
        "map": {
          "socketio": "@empty"
        }
      },
      "server-production": {
        "baseURL": "http://matt-pmo.divshot.io/",
        "map": {
          "socketio": "@empty"
        }
      }
    },
    "meta": {
      "socketio": {
        "format": "global"
      }
    },
    "map": {
      "bit-tabs/tabs.less!$less": "@empty"
    },
    "paths": {
      "socketio": "node_modules/socket.io-client/socket.io.js"
    },
    "configDependencies": [
      "live-reload"
    ],
    "bundle": [
      "pmo/home.component!",
      "pmo/order/new/",
      "pmo/order/list.component!",
      "pmo/restaurant/details.component!",
      "pmo/restaurant/list/list",
      "pmo/order/history.component!"
    ],
    "npmIgnore": [
      "steal-cordova",
      "steal-nw",
      "documentjs",
      "testee",
      "place-my-order-api",
      "donejs-deploy",
      "testee"
    ]
  },
  "window": {
    "width": 1060,
    "height": 625,
    "toolbar": false
  }
}
