{
  "name": "place-my-order",
  "version": "0.1.0",
  "description": "The place-my-order frontend, built with DoneJS",
  "main": "nw.html",
  "repository": "git@github.com:donejs/place-my-order.git",
  "scripts": {
    "document": "documentjs",
    "start": "node .server.js",
    "test": "testee test.html --browsers firefox --reporter Spec",
    "live-reload": "steal-tools live-reload",
    "build": "node build",
    "build:cordova": "node build cordova",
    "build:nw": "node build nw"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "bit-tabs": "0.1.0-pre.0",
    "can": "2.3.0-pre.1",
    "can-connect": "0.0.2",
    "can-ssr": "^0.3.1",
    "d3": "3.5.5",
    "done-autorender": "^0.1.0-pre.2",
    "done-css": "^0.0.8",
    "es6-promise": "^2.2.0",
    "jquery": "^1.11.3",
    "place-my-order-api": "^0.4.2",
    "place-my-order-assets": "^0.1.0",
    "socket.io-client": "^1.3.5",
    "steal": "^0.10.4",
    "steal-platform": "0.0.4",
    "done-component": "^0.1.0"
  },
  "devDependencies": {
    "documentjs": "^0.3.0-pre.5",
    "funcunit": "^3.0.0",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "steal-qunit": "0.0.4",
    "steal-tools": "^0.11.0-pre.2",
    "steal-cordova": "0.0.5",
    "steal-nw": "0.0.2",
    "testee": "^0.2.0"
  },
  "system": {
    "main": "pmo/index.stache!done-autorender",
    "transpiler": "babel",
    "meta": {
      "socketio": {
        "format": "global"
      },
      "can/util/vdom/vdom": {
        "sideBundle": true
      }
    },
    "map": {
      "bit-tabs@0.0.4#tabs.less!$less": "@empty",
      "$css": "done-css"
    },
    "paths": {
      "socketio": "node_modules/socket.io-client/socket.io.js"
    },
    "configDependencies": [
      "live-reload",
      "system-config"
    ],
    "bundle": [
      "pmo/home.component!",
      "pmo/order/new/",
      "pmo/order/list.component!",
      "pmo/restaurant/details.component!",
      "pmo/restaurant/list/list",
      "pmo/order/history.component!"
    ],
    "npmIgnore": [
      "steal-cordova",
      "steal-nw",
      "documentjs",
      "testee",
      "place-my-order-api"
    ]
  },
  "window": {
    "width": 1060,
    "height": 625,
    "toolbar": false
  }
}
